{
	"name": "My Cool Extension",
	"content_scripts": [
		{
			"matches": ["https://rolz.org/dr?*"],
			"js": [
				"jquery-2.2.4.min.js",
				"distribution.js",
				"node_modules/nearley/lib/nearley.js",
				"grammar.js",
				"parse.js",
				"roll.js",
				"RollMessage.js",
				"content.js"
			]
		}
	],
	"browser_action": {},
	"background": {
		"description": "desc",
		"scripts": ["background.js"]
	},
	"commands": {
		"_execute_browser_action": {
			"suggested_key": {
				"default": "Ctrl+Shift+A",
				"mac": "Command+Shift+A"
			},
			"description": "runs the button"
		}
	},
	"manifest_version": 2,
	"version": "0.2"
}
