{
	"scripts": {
		"lint": "eslint --fix **/*.js",
		"format-json": "prettier --write **/*.json",
		"compile-parser": "nearleyc grammar.ne -o grammar.js",
		"test": "jasmine",
		"testmon": "nodemon --exec  jasmine"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"grammar.ne": "nearleyc -o grammar.js",
		"*.js": [
			"eslint  --fix",
			"jasmine --fail-fast=true spec/tests/*.js"
		],
		"*.json": "prettier --write"
	},
	"devDependencies": {
		"adm-zip": "^0.5.4",
		"eslint": "^7.20.0",
		"eslint-config-prettier": "^7.2.0",
		"eslint-plugin-prettier": "^3.3.1",
		"husky": "^4.3.8",
		"jasmine": "^3.6.4",
		"jsdom": "^16.5.1",
		"lint-staged": "^10.5.4",
		"nodemon": "^2.0.7",
		"nyc": "^15.1.0",
		"prettier": "^2.2.1"
	},
	"dependencies": {
		"nearley": "^2.20.1"
	}
}
